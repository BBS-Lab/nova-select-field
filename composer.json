{
    "name": "bbs-lab/nova-select-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "BBSLab\\NovaSelectField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BBSLab\\NovaSelectField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
